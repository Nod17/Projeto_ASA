;
; BIND data file for local loopback interface
;
$TTL	604800
@	IN	SOA	ac.asa.br. root.ac.asa.br. (
			      5		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	NS	ac.asa.br.
@	IN	A	10.24.10.105
ruth	IN	A	10.25.2.134
www	IN	CNAME	ac.asa.br.
@	IN	A	127.0.0.1
@	IN	AAAA	::1

ns1			A	10.24.10.105		; Change to desired NS1 IP
www			CNAME	ns1		; Change to desired NS2 IP