; BIND data file for local loopback interface
;
$TTL	604800
@	IN	SOA	ac.asa.br. root.ac.asa.br. (
			      1		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	NS	ac.asa.br.
@	IN	NS	ns1.ac.asa.br.
@	IN	NS	ns2.ac.asa.br.
@	IN	A	primary

ns1	IN	A	primary
;ns2	IN	A	secondary
