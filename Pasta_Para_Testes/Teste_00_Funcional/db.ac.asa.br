;
; BIND data file for local loopback interface
;
$TTL	604800
@	IN	SOA	ac.asa.br. root.ac.asa.br. (
			      3		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	NS	ac.asa.br.

@	IN	A	127.0.0.1
@	IN	AAAA	::1

ns1		A	172.17.0.2
;ns2		A	172.17.0.3

